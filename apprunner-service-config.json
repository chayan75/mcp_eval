{
    "ServiceName": "mcp-eval-server",
    "SourceConfiguration": {
        "ImageRepository": {
            "ImageIdentifier": "682033472096.dkr.ecr.us-east-1.amazonaws.com/mcp-eval-server:latest",
            "ImageConfiguration": {
                "Port": "8080",
                "RuntimeEnvironmentVariables": {
                    "PYTHONUNBUFFERED": "1",
                    "PYTHONDONTWRITEBYTECODE": "1",
                    "MCP_EVAL_CACHE_DIR": "/tmp/cache",
                    "MCP_EVAL_RESULTS_DB": "/tmp/results/evaluation_results.db",
                    "DEFAULT_JUDGE_MODEL": "gpt-4o-mini",
                    "PORT": "8080",
                    "MCP_PORT": "9001"
                }
            },
            "ImageRepositoryType": "ECR"
        },
        "AuthenticationConfiguration": {
            "AccessRoleArn": "arn:aws:iam::682033472096:role/AppRunnerServiceRole"
        },
        "AutoDeploymentsEnabled": true
    },
    "InstanceConfiguration": {
        "Cpu": "0.25 vCPU",
        "Memory": "0.5 GB",
        "InstanceRoleArn": "arn:aws:iam::682033472096:role/AppRunnerInstanceRole"
    },
    "HealthCheckConfiguration": {
        "Protocol": "HTTP",
        "Path": "/health",
        "Interval": 10,
        "Timeout": 10,
        "HealthyThreshold": 1,
        "UnhealthyThreshold": 5
    }
}
